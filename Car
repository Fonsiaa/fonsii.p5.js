function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(255);
  noStroke();
  
  //rooftop
  fill ("#242424")
  beginShape ();
  vertex(294,161);
  bezierVertex (272,131,259,109,256,109,251,104);
