  bezierVertex (145,241,143,233,141,227,138,220);
  bezierVertex (125,209,131,203,127,197,122,195);
  //going down the front tire
  bezierVertex (118,205,116,209,115,215,115,221);
  bezierVertex (115,228,115,235,116,244,116,248);
  bezierVertex (126,248,116,254,116,259,57,245);
  //backside tire 
  bezierVertex (55,240,54,233,52,227,52,222);
  bezierVertex (51,216,49,211,48,203,45,196);
  bezierVertex (41,192,32,186,33,210,32,209);
  bezierVertex (31,198,33,185,37,176,329,175);
  endShape();
   
  //body of the car
  fill("grey");
